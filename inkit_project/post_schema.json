{
  "$id": "http://example.com/example.json",
  "type": [
    "object",
    "array"
  ],
  "properties": {
    "first_name": {
      "$id": "/properties/first_name",
      "type": "string",
      "title": "The First_name Schema ",
      "default": "",
      "examples": [
        "Juan David"
      ]
    },
    "last_name": {
      "$id": "/properties/last_name",
      "type": "string",
      "title": "The Last_name Schema ",
      "default": "",
      "examples": [
        "Garrido"
      ]
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "title": "The Email Schema ",
      "default": "",
      "examples": [
        "jgarrido@macalester.edu"
      ]
    },
    "phone_number": {
      "$id": "/properties/phone_number",
      "type": [
        "string",
        "null"
      ],
      "title": "The Phone_number Schema ",
      "default": "",
      "examples": [
        "7632830994"
      ]
    },
    "company": {
      "$id": "/properties/company",
      "type": [
        "string",
        "null"
      ],
      "title": "The Company Schema ",
      "default": "",
      "examples": [
        "Inkit"
      ]
    },
    "address": {
      "$id": "/properties/address",
      "type": "object",
      "properties": {
        "street_address": {
          "$id": "/properties/address/properties/street_address",
          "type": "string",
          "title": "The Street_address Schema ",
          "default": "",
          "examples": [
            "1600 Grand Avenue"
          ]
        },
        "unit_number": {
          "$id": "/properties/address/properties/unit_number",
          "type": [
            "string",
            "null"
          ],
          "title": "The Unit_number Schema ",
          "default": "",
          "examples": [
            "Macalester College"
          ]
        },
        "city": {
          "$id": "/properties/address/properties/city",
          "type": "string",
          "title": "The City Schema ",
          "default": "",
          "examples": [
            "St. Paul"
          ]
        },
        "state": {
          "$id": "/properties/address/properties/state",
          "type": "string",
          "title": "The State Schema ",
          "default": "",
          "examples": [
            "MN"
          ]
        },
        "country": {
          "$id": "/properties/address/properties/country",
          "type": [
            "string",
            "null"
          ],
          "title": "The Country Schema ",
          "default": "US",
          "examples": [
            "Colombia"
          ]
        },
        "post_code": {
          "$id": "/properties/address/properties/post_code",
          "type": "string",
          "title": "The Post_code Schema ",
          "default": "",
          "examples": [
            "55105"
          ]
        }
      },
      "required": [
        "street_address",
        "city",
        "state",
        "post_code"
      ]
    },
    "notes": {
      "$id": "/properties/notes",
      "type": [
        "string",
        "null"
      ],
      "title": "The Notes Schema ",
      "default": "",
      "examples": [
        "This is a difficult project"
      ]
    }
  },
  "required": [
    "first_name",
    "last_name",
    "email",
    "address"
  ]
}